                        }
                        else
                        {
                            strMsg = strMsg + "<td>" + row[col.Ordinal].ToString() + "</td>";
                        }
                    }

                    /// Don't return more than 10 rows.
                    icounter++;
                    if (icounter >= 10)
                    {
                        strMsg = strMsg + "<tr colspan=" + col.Ordinal + "><td>...</td></tr>";
                        break;
                    }
                }
                strMsg = strMsg + "</table>";
